{
  "name": "seekho-backend",
  "version": "1.0.0",
  "description": "Learning video app backend with subscription system",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "seed": "node seeds/seedData.js",
    "test": "jest",
    "test-api": "node scripts/test-api.js",
    "setup": "chmod +x scripts/setup.sh && ./scripts/setup.sh",
    "setup-oauth": "chmod +x scripts/setup-google-oauth.sh && ./scripts/setup-google-oauth.sh",
    "kill-port": "chmod +x scripts/kill-port.sh && ./scripts/kill-port.sh",
    "dev-clean": "npm run kill-port 3001 && npm run dev"
  },
  "keywords": ["learning", "video", "subscription", "education"],
  "author": "Seekho Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.41.0",
    "razorpay": "^2.9.2",
    "stripe": "^14.7.0",
    "dotenv": "^16.3.1",
    "morgan": "^1.10.0",
    "compression": "^1.7.4",
    "express-mongo-sanitize": "^2.2.0",
    "xss-clean": "^0.1.4",
    "hpp": "^0.2.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "aws-sdk": "^2.1490.0",
    "multer-s3": "^3.0.1",
    "google-auth-library": "^9.4.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
